ClosestPlrFromMouse = function()
    local Target, Closest = nil, 1 / 0

    for _, v in pairs(Players:GetPlayers()) do
        if (v.Character and v ~= Client and v.Character:FindFirstChild("HumanoidRootPart")) then
            local Position, OnScreen = Camera:WorldToScreenPoint(v.Character.HumanoidRootPart.Position)
            local Distance = (Vector2.new(Position.X, Position.Y) - Vector2.new(Mouse.X, Mouse.Y)).Magnitude

            if (Circle.Radius > Distance and Distance < Closest and OnScreen) then
                Closest = Distance
                Target = v
            end
        end
    end
    return Target
end
local V2 = Vector2
local f = find

local WTS = function(Object)
    local ObjectVector = Camera:WorldToScreenPoint(Object.Position)
    return V2.new(ObjectVector.X, ObjectVector.Y)
end

local IsOnScreen = function(Object)
    local IsOnScreen = Camera:WorldToScreenPoint(Object.Position)
    return IsOnScreen
end

local FilterObjs = function(Object)
    if string.find(Object.Name, "Gun") then
        return
    end
    if table.f({"Part", "MeshPart", "BasePart"}, Object.ClassName) then
        return true
    end
end

local LOOOL = game.Workspace.CurrentCamera
local DICK = game.Players.LocalPlayer:GetMouse()

function GetClosestPLR()

    local closestPlayer
    local SuckAniggaDick = math.huge

    for i, v in pairs(game.Players:GetPlayers()) do
        pcall(
            function()
                if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("Humanoid") then
                    local pos = LOOOL:WorldToViewportPoint(v.Character.PrimaryPart.Position)
                    local magnitude = (Vector2.new(pos.X, pos.Y) - Vector2.new(DICK.X, DICK.Y)).magnitude
                    if (Vector2.new(pos.X, pos.Y) - Vector2.new(DICK.X, DICK.Y)).magnitude < SuckAniggaDick then
                        closestPlayer = v
                        SuckAniggaDick = magnitude
                    end
                end
            end
        )
    end
    return closestPlayer
end

local Monkey = math.huge
local SUCKANIGGERDICKKKK = nil

function GetBodyPartClosest(character)

    if (character and character:GetChildren()) then
        for k, x in next, character:GetChildren() do
            if Filter(x) and IsOnScreen(x) then
                local Distance = (WTS(x) - Vector2.new(Mouse.X, Mouse.Y)).Magnitude

                if Distance < Monkey then
                    Monkey = Distance
                    SUCKANIGGERDICKKKK = x
                end
            end
        end
    end

    return SUCKANIGGERDICKKKK
end

